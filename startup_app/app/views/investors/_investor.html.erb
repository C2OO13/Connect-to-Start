<% obj.each do |investor| %>
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <div class= "card-title h5">
        <%= investor.user.fname.upcase%> <%=investor.user.lname.upcase %>
      </div>
      <p><%#= investor.description, class: "card-text" %></p>
    </div>
    <ul class="list-group list-group-flush">
      <li class="list-group-item"><%= investor.user.email %></li>
      <li class="list-group-item"><%= investor.user.phone %></li>
      <li class="list-group-item"><%= investor.user.dob %></li>
    </ul>
<!--    <div class="card-body">-->
<!--      <a href="#" class="card-link">Card link</a>-->
<!--      <a href="#" class="card-link">Another link</a>-->
<!--    </div>-->

    
  </div>
<% end %>